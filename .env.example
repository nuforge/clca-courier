# Environment Variables for Local Development
# Copy this file to .env and add your actual values

# Firebase Configuration
# Get these values from your Firebase Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Application Configuration
VITE_APP_URL=http://localhost:9000
VITE_API_URL=http://localhost:9000/api

# PDFTron WebViewer License Key
# Get your license key from: https://dev.apryse.com/
# VITE_PDFTRON_LICENSE_KEY=your_license_key_here

# Canva Connect API Configuration
# Get these values from your Canva Connect app configuration
VITE_CANVA_API_BASE_URL=https://api.canva.com/rest/v1
VITE_CANVA_APP_ID=your_canva_app_id_here
VITE_CANVA_API_REDIRECT_URI=https://yourdomain.com/canva/callback

# Legacy Canva integration (if using)
VITE_CANVA_API_KEY=your_canva_api_key
VITE_CANVA_BRAND_TEMPLATE_ID=your_brand_template_id

# Example Firebase values (replace with your actual credentials):
# VITE_FIREBASE_API_KEY=AIzaSyBkHZ8Q7j2X8kL3mN5oP9qR7sT2uV6wX8y
# VITE_FIREBASE_AUTH_DOMAIN=clca-courier.firebaseapp.com
# VITE_FIREBASE_PROJECT_ID=clca-courier
# VITE_FIREBASE_STORAGE_BUCKET=clca-courier.appspot.com
# VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
# VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012345
# VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ
