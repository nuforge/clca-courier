# PDFTron WebViewer License Key
# Get your license key from: https://dev.apryse.com/
# VITE_PDFTRON_LICENSE_KEY=your_license_key_here

# Google Cloud API Configuration
# Get your credentials from: https://console.cloud.google.com/

# Google Drive API Key (from Google Cloud Console)
VITE_GOOGLE_API_KEY=your_api_key_here

# Google OAuth2 Client ID (from Google Cloud Console)
VITE_GOOGLE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# Optional: Google OAuth2 Client Secret (for server-side apps)
VITE_GOOGLE_CLIENT_SECRET=your_client_secret_here

# Optional: Google Cloud Project ID
VITE_GOOGLE_PROJECT_ID=your_project_id_here

# Optional: Default Google Drive Public Folder ID for submissions
# Current public folder: https://drive.google.com/drive/folders/1saSXnh9kkD_KNVwqusaz3i9YP46NZmIz?usp=sharing
VITE_GOOGLE_DRIVE_PUBLIC_FOLDER_ID=1saSXnh9kkD_KNVwqusaz3i9YP46NZmIz

# Google Drive Folder IDs for organized content
# Create these folders in your Google Drive and get their IDs from the URL
VITE_GOOGLE_DRIVE_CONTENT_FOLDER_ID=your_content_folder_id
VITE_GOOGLE_DRIVE_ISSUES_FOLDER_ID=your_issues_folder_id
VITE_GOOGLE_DRIVE_IMAGES_FOLDER_ID=your_images_folder_id
VITE_GOOGLE_DRIVE_TEMPLATES_FOLDER_ID=your_templates_folder_id

# Sync settings
VITE_GOOGLE_DRIVE_SYNC_INTERVAL_MINUTES=30
VITE_GOOGLE_DRIVE_CACHE_MAX_AGE_DAYS=7

# Canva integration (if using)
VITE_CANVA_API_KEY=your_canva_api_key
VITE_CANVA_BRAND_TEMPLATE_ID=your_brand_template_id

# Example values (replace with your actual credentials):
# VITE_GOOGLE_API_KEY=AIzaSyBkHZ8Q7j2X8kL3mN5oP9qR7sT2uV6wX8y
# VITE_GOOGLE_CLIENT_ID=123456789012-abcdefghijklmnopqrstuvwxyz123456.apps.googleusercontent.com
